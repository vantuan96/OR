<div class="modal fade modal_booking" id="user_unlock" aria-hidden="true" data-backdrop="static" style="display: none;">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">×</button>
                <h2 class="modal-title title-popup">@LayoutResource.UserMngt_PopupUnlock_Title</h2>
                <br />
            </div>
            <div class="modal-body">
                <div class="body_booking">
                    <div class="dis_tbl">
                        <p class=" dis_tbl_r" style="margin-top: 10px;">
                            <strong class="dis_tbl_c text-right">@LayoutResource.UserMngt_ListUser_Label_UserFullName</strong>&nbsp;
                            <span class="dis_tbl_c"><span class="user_fullname"></span></span>
                        </p>
                        <p class=" dis_tbl_r" style="margin-top: 10px;">
                            <strong class="dis_tbl_c text-right">@LayoutResource.UserMngt_ListUser_Label_EmailAddress</strong>&nbsp;
                            <span class="dis_tbl_c user_email"></span>
                        </p>
                    </div>
                    <p class="desc null">@Html.Raw(LayoutResource.UserMngt_PopupUnlock_Description)</p>
                    <div class="btn-wrapper text-right">
                        <button class="btn btn-default btn-submit" type="submit" tabindex="7"><i class="fa fa fa-unlock"></i>&nbsp;@LayoutResource.Shared_BtnText_Unlock</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
